{
	"name": "goodcase",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"client": "vite dev --host",
		"build": "npx prisma generate && vite build",
		"postinstall": "prisma generate",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"dbpush": "npx prisma db push && npx prisma generate"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.1",
		"@skeletonlabs/skeleton": "^2.1.0",
		"@skeletonlabs/tw-plugin": "^0.2.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.17.7",
		"@types/node": "^18.17.16",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"nodemon": "^3.1.0",
		"postcss": "^8.4.21",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.16.2",
		"svelte": "^3.54.0",
		"svelte-carousel": "github:vadimkorr/svelte-carousel#pull/142/head",
		"svelte-check": "^3.0.1",
		"sveltekit-superforms": "^1.7.0",
		"tailwindcss": "^3.3.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"vite": "^4.2.0",
		"vitest": "^0.25.3",
		"zod": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.2.6",
		"@prisma/client": "^4.13.0",
		"concurrently": "^8.2.1",
		"cookie-session": "^2.0.0",
		"cors": "^2.8.5",
		"ejs": "^3.1.9",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"mongodb": "^5.7.0",
		"motion": "^10.17.0",
		"openid-client": "^5.4.2",
		"parse-nested-form-data": "^1.0.0",
		"svelte-sound": "^0.6.0"
	}
}
